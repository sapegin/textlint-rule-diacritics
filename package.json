{
  "name": "textlint-rule-diacritics",
  "version": "0.0.0-development",
  "description": "Textlint rule to check correct usage of diacritics",
  "author": {
    "name": "Artem Sapegin",
    "url": "http://sapegin.me"
  },
  "homepage": "https://github.com/sapegin/textlint-rule-diacritics",
  "repository": "sapegin/textlint-rule-diacritics",
  "license": "MIT",
  "engines": {
    "node": ">=8.9"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "words.json"
  ],
  "scripts": {
    "try": "textlint --rule ./index.js Readme.md",
    "pretest": "npm run lint",
    "test": "npm run test:jest",
    "posttest": "npm run format",
    "lint": "eslint . --cache --fix",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"**/*.{js,md}\""
  },
  "keywords": [
    "textlintrule",
    "textlint",
    "rule",
    "misspelling",
    "proofreading",
    "diacritic",
    "diacritics",
    "diacritical",
    "mark",
    "point",
    "sign"
  ],
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-tamia": "^7.1.1",
    "jest": "^29.3.1",
    "prettier": "^1.19.1",
    "textlint-tester": "^5.1.13"
  },
  "dependencies": {
    "match-casing": "^1.0.2",
    "strip-json-comments": "^3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
